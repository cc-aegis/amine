fn fib(self: &Thread) {
    let x = 0;
    let y = 1;
    let terminal = Thread::set_terminal_mode(self, true);
    loop {
        let z = x + y;
        x = y;
        y = z;
        terminal::write_u16(x);
        terminal::write_char('\n');
        Thread::next();
    }
}